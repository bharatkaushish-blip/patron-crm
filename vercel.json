{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.patroncollective.com" }],
      "destination": "https://patroncollective.com/:path*",
      "permanent": true
    }
  ],
  "crons": [
    {
      "path": "/api/cron/reminders",
      "schedule": "0 3 * * *"
    }
  ]
}
